car_0={
    'model':'lasetti',
    'rangi':'oq',
    'yil':'2018',
    'narh':'13000',
    'km':'50000',
    'korobka':'avtomat' 
    }    
car_1={
    'model':'nexia',
    'rangi':'qora',
    'yil':'2019',
    'narh':'7000',
    'km':'100000',
    'korobka':'mexanika'
    }
car_2={
    'model':'BMW',
    'rangi':'qora',
    'yil':'2020',
    'narh':'120000',
    'km':'0',
    'korobka':'avtomat' 
}
cars = [car_0, car_1, car_2]
for car in cars:
    print(f"{car['model'].title()}, "
          f"{car['rangi']} rang, "
          f"{car['yil']}-yil, {car['narh']}$")
print(cars[0])
print(cars[0]['model'])
print(cars[2]['model'])
print("\n")
print(f"{cars[2]['rangi'].title()} "
      f"{cars[2]['model']}")
