# LUG'ATNI QATORLARGA BO'LIB YOZISH

telefonlar={
  'ali':'iphona XS',
  'olim':'oppo',
  'vika':'vivo',
  'mucci':'samsung',
  'komil':'xiaomi'
}
print(telefonlar)

# .get() METODI

phone=telefonlar['ali']
print(f"Alining telefoni {phone}")

phone=telefonlar.get('alim', 'bunfay ism yuq')
print(phone)
phone=telefonlar.get('hasan')
print(phone)
